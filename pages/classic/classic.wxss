.container{
    /*border: 1px solid red;*/
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 100%;
}

.header{
    display: flex;
    width: 100%;
    justify-content: space-between;
    height: 100rpx;
    border-top: 1px solid #f5f5f5;
    border-bottom: 1px solid #f5f5f5;
}
.header .date{
    display: flex;
    align-items: center;
    justify-content: center;
    margin-left: 20rpx;
    margin-top: 4rpx;
}
.header .like{
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 6rpx;
    /*border: 1px solid red;*/
}

.container .navi{
    position: absolute;
    bottom: 40rpx;

}

